<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.auribises.myapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/contact"
        android:label="@string/app_name"
        android:roundIcon="@drawable/contact"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityOne" />
        <activity android:name=".ActivityTwo">
            <intent-filter>
                <action android:name="com.auribises.myapp.ActivityTwo" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".WebViewActivity" />
        <activity android:name=".NewsActivity" />
        <activity android:name=".EnquiryActivity" />
        <activity android:name=".MyCustomListActivity"></activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyFragmentsActivity"></activity>
    </application>

</manifest>